<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8" />
		<title>{% block title %}| Onix Game{% endblock %}</title>
		<!-- <link rel="shortcut icon" href="" /> -->
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
		<link href="https://fonts.googleapis.com/css?family=Roboto:400,600,700" rel="stylesheet">

		{% block stylesheets %}
			<link href="https://use.fontawesome.com/releases/v5.3.1/css/all.css" rel="stylesheet">
			<link rel="stylesheet" href="assets/css/bootstrap.css" />
			<link rel="stylesheet" href="assets/bs/common.css" />
			<link rel="stylesheet" href="assets/bs/boards/board.css" />
			<link rel="stylesheet" href="assets/bs/pieces/external.css" />
			<link rel="stylesheet" href="assets/css/chess-online.css">
		{% endblock stylesheets %}
		{% block head %}
		<script>
        	var app_props = {
            	channel: '',
            	modules: []
        	};
    	</script>
		{% endblock head %}
	</head>
	<body>
		<div class="wrapper {{ wrapperClass }}">
			{% block content %}
			{% endblock content %}
			<div id="app-root"></div>
		</div>
		{% block scripts %}
		<script src="https://code.jquery.com/jquery-3.6.0.js"></script>
		<script src="assets/js/bootstrap.js"></script>
		<script src="assets/js/chess-online.manifest.js"></script>
    	<script src="assets/js/chess-online.vendors.js"></script>
    	<script src="assets/js/chess-online.site.js"></script>
		{% endblock scripts %}

		<script>
			jQuery(function () {
				onix.Logger.setLevel(9);
				onix.ChessApp(app_props, document.getElementById("app-root")); 
			});
		</script>
	</body>
</html>

{
  "name": "onix-chess-online",
  "version": "2.0.0",
  "description": "Chess-Online client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "serve": "vite",
    "dev": "cross-env NODE_ENV=development gulp site",
    "script:dev": "cross-env NODE_ENV=development gulp site:script",
    "style:dev": "cross-env NODE_ENV=development gulp site:style",
    "preprod": "node -p \"'export const APP_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/app/Version.ts",
    "prod": "cross-env NODE_ENV=production gulp site",
    "script:prod": "cross-env NODE_ENV=production gulp site:script",
    "style:prod": "cross-env NODE_ENV=production gulp site:style",
    "prepare": "husky",
    "prepublishOnly": "npm run prod && npm run dev",
    "lint": "eslint ./src/js --cache --report-unused-disable-directives --ext .js,.ts,.tsx --max-warnings 0",
    "lint:ci": "eslint ./src/js --report-unused-disable-directives --ext .js,.ts,.tsx --max-warnings 0",
    "precommit": "lint-staged",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest unittest.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DrNixx/onix-chess-online.git"
  },
  "keywords": [
    "onix",
    "chess",
    "board"
  ],
  "author": "Nick Panteleeff <drnixx@gmail.com> (https://github.com/DrNixx)",
  "license": "GPL-3.0",
  "bugs": {
